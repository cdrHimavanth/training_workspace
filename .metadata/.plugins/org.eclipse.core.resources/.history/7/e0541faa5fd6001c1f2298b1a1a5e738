package com.service;

import com.model.Employee;

public interface EmployeeWorks {
	public abstract Employee[] addEmployee(Employee[] emps, Employee emp);
	public abstract Employee[] removeEmployee(Employee[] emps, int empId);
	public abstract Employee[] updateEmployee(Employee[] emps, int empId,String name,float salary);
	public abstract void readAllEmployees(Employee[] emps);
}
